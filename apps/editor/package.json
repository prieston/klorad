  "private": true,
  "engines": {
    "node": ">=20 <23",
    "pnpm": ">=9"
  },
  "scripts": {
    "lint": "next lint --fix --dir app",
    "dev": "NODE_ENV=development NEXT_TELEMETRY_DISABLED=1 next dev -p 3001",
